v0x55e882f7d5e0_0 .var "p", 3 0;
v0x55e882f7d6c0_0 .var "sum", 4 0;
E_0x55e882f4fe50 .event posedge, v0x55e882f7d3f0_0;
    .scope S_0x55e882f5d060;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x55e882f7d300_0, 4, 1;
    %end;
    .thread T_0;
    .scope S_0x55e882f5d060;
T_1 ;
    %wait E_0x55e882f4fe50;
    %load/vec4 v0x55e882f5d2a0_0;
    %load/vec4 v0x55e882f7d220_0;
    %xor;
    %store/vec4 v0x55e882f7d5e0_0, 0, 4;
    %load/vec4 v0x55e882f5d2a0_0;
    %load/vec4 v0x55e882f7d220_0;
    %and;
    %store/vec4 v0x55e882f7d4b0_0, 0, 4;
    %load/vec4 v0x55e882f7d4b0_0;
