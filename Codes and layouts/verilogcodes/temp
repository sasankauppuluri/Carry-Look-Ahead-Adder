S_0x55e882f5cee0 .scope module, "adder_tb" "adder_tb" 2 2;
 .timescale -9 -12;
v0x55e882f7d820_0 .var "a", 3 0;
v0x55e882f7d900_0 .var "b", 3 0;
v0x55e882f7d9d0_0 .var "clk", 0 0;
v0x55e882f7dad0_0 .net "sum", 4 0, v0x55e882f7d6c0_0;  1 drivers
S_0x55e882f5d060 .scope module, "uut" "adder" 2 8, 3 3 0, S_0x55e882f5cee0;
