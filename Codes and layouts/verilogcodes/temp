    .port_info 0 /INPUT 1 "clock"
    .port_info 1 /INPUT 4 "a"
    .port_info 2 /INPUT 4 "b"
    .port_info 3 /OUTPUT 5 "sum"
v0x55e882f5d2a0_0 .net "a", 3 0, v0x55e882f7d820_0;  1 drivers
v0x55e882f7d220_0 .net "b", 3 0, v0x55e882f7d900_0;  1 drivers
