    %flag_set/imm 4, 0;
    %store/vec4 v0x55e882f7d300_0, 4, 1;
    %load/vec4 v0x55e882f7d4b0_0;
    %parti/s 1, 2, 3;
    %load/vec4 v0x55e882f7d5e0_0;
    %parti/s 1, 2, 3;
    %load/vec4 v0x55e882f7d4b0_0;
    %parti/s 1, 1, 2;
    %and;
    %or;
    %load/vec4 v0x55e882f7d5e0_0;
    %parti/s 1, 2, 3;
    %load/vec4 v0x55e882f7d5e0_0;
    %parti/s 1, 1, 2;
    %and;
    %load/vec4 v0x55e882f7d5e0_0;
    %parti/s 1, 0, 2;
    %and;
    %or;
